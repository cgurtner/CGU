RewriteEngine on

<Files "config.json">
Order Allow,Deny
Deny from all
</Files>

RewriteRule ^gui/.* - [L]
RewriteRule ^files/.* - [L]
RewriteRule ^vendor/.* - [L]
RewriteRule ^robots.txt$ - [L]

RewriteRule ^sitemap.xml$ sitemap.php [L]
RewriteRule ^sitemap.php$ - [L]

RewriteRule (.*) index.php?page=$1&%{QUERY_STRING} [L]
